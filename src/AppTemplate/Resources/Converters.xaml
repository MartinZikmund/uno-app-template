<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:AppTemplate.Converters">

    <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
    <converters:BoolToVisibilityConverter x:Key="InvertedBoolToVisibilityConverter" Invert="True" />
    <converters:BoolNegationConverter x:Key="BoolNegationConverter" />
    <converters:NullToVisibilityConverter x:Key="NullToVisibilityConverter" />
    <converters:NullToVisibilityConverter x:Key="InvertedNullToVisibilityConverter" Invert="True" />
    <converters:StringToVisibilityConverter x:Key="StringToVisibilityConverter" />
    <converters:StringToVisibilityConverter x:Key="InvertedStringToVisibilityConverter" Invert="True" />
    <converters:EnumLocalizationConverter x:Key="EnumLocalizationConverter" />

</ResourceDictionary>
